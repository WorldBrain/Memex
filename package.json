{
    "name": "worldbrain-extension",
    "version": "3.17.0",
    "homepage": "https://memex.garden",
    "repository": "https://github.com/WorldBrain/Memex",
    "scripts": {
        "build": "npm run build:clean && webpack",
        "build:ci": "npm run build:clean && webpack --env.ci",
        "build:prod": "npm run build:clean && webpack --env.prod",
        "build:prod:mv2": "npm run build:clean && MANIFEST_VERSION=2 webpack --env.prod",
        "build:package": "npm run build:clean && webpack --env.prod --env.package",
        "build:package:mv2": "npm run build:clean && MANIFEST_VERSION=2 webpack --env.prod --env.package",
        "build:package-sentry:mv2": "npm run build:clean && MANIFEST_VERSION=2 webpack --env.prod --env.package --env.sentry",
        "build:package-sentry": "npm run build:clean && webpack --env.prod --env.package --env.sentry",
        "build:clean": "rimraf ./extension",
        "watch": "npm run build:clean && webpack --watch",
        "watch:prod:mv2": "npm run build:clean && MANIFEST_VERSION=2 webpack --watch --env.prod",
        "watch:notif:mv2": "npm run build:clean && MANIFEST_VERSION=2 webpack --watch --env.notifs",
        "watch:prod": "npm run build:clean && webpack --watch --env.prod",
        "watch:notif": "webpack --watch --env.notifs",
        "watch:clean": "npm run cache:clean && npm run watch:notif",
        "watch:clean:mv2": "npm run cache:clean && npm run watch:notif:mv2",
        "watch:fb-emu": "USE_FIREBASE_EMULATOR=true yarn watch:clean:mv2",
        "cache:clean": "rimraf ./node_modules/.cache",
        "stats": "webpack --profile -1json > stats.json",
        "postgres:start": "yarn postgres stop; sudo docker run --name storex-test-postgres -p 127.0.0.1:5435:5432/tcp -e POSTGRES_PASSWORD=storex -d postgres",
        "postgres:stop": "sudo docker rm -f storex-test-postgres",
        "publish-chrome": "node publish --chrome",
        "publish-ff": "node publish --ff",
        "mocha": "TS_NODE_PROJECT=tsconfig.jest.json mocha --require ts-node/register -r tsconfig-paths/register",
        "test": "jest --verbose --silent --config=.jest-config.js",
        "test-nosync": "SKIP_SYNC_TESTS=true node --async-stack-traces node_modules/.bin/jest --verbose --silent --config=.jest-config.js",
        "test:watch": "node --async-stack-traces node_modules/.bin/jest --watch --config=.jest-config.js",
        "test:watch:fb-emu": "TEST_SERVER_STORAGE=firebase-emulator DISABLE_FIRESTORE_RULES=true yarn test:watch",
        "test:watch:fb-emu-rules": "TEST_SERVER_STORAGE=firebase-emulator yarn test:watch",
        "test:watch:debugger": "node --inspect --async-stack-traces node_modules/.bin/jest --watch --no-cache --runInBand --config=.jest-config.js",
        "test-nosync:watch": "SKIP_SYNC_TESTS=true jest --watch --config=.jest-config.js",
        "test:coverage": "jest --silent --coverage --config=.jest-config.js",
        "test:mocha": "TS_NODE_PROJECT=tsconfig.jest.json mocha --require ts-node/register -r tsconfig-paths/register 'src/backup-restore/background/**/*.test.ts' 'src/util/webextensionRPC.test.ts' 'src/util/webextensionRPC.test.ts' 'src/sync/**/*.test.ts' 'src/tests/**/*.test.ts'",
        "test:mocha:watch": "TS_NODE_PROJECT=tsconfig.jest.json mocha -r source-map-support/register -r ts-node/register -r tsconfig-paths/register 'src/backup-restore/background/**/*.test.ts' 'src/util/webextensionRPC.test.ts' 'src/sync/**/*.test.ts' 'src/tests/**/*.test.ts' --watch --watch-extensions ts",
        "test:browser": "TS_NODE_PROJECT=tsconfig.jest.json mocha --require ts-node/register -r tsconfig-paths/register src/**/*.browser-test.ts",
        "lint": "npm-run-all --parallel lint:*",
        "lint:ts": "tslint --project . --format stylish",
        "lint:js": "eslint --ext js --ext jsx src",
        "lint:css": "stylelint 'src/**/*.css'",
        "format": "prettier --config prettier.config.js --write '**/*.{ts,js,tsx,jsx,css}'",
        "storybook": "start-storybook -p 9001 -c .storybook",
        "storybook:ci": "start-storybook -c .storybook -s ./extension --ci --smoke-test",
        "storage-explorer": "TS_NODE_PROJECT=tsconfig.jest.json ts-node -r tsconfig-paths/register src/tools/storage-explorer.ts"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
